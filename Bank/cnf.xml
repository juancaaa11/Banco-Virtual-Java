<?xml version="1.0" encoding="UTF-8"?>
<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>gui</headerType>
  
  <!-- Ruta relativa al JAR -->
  <jar>Bank.jar</jar>
  
  <!-- Ruta relativa al EXE de salida -->
  <outfile>Bank.exe</outfile>
  
  <errTitle></errTitle>
  <cmdLine></cmdLine>
  <chdir>.</chdir>
  <priority>high</priority>
  <downloadUrl>https://www.oracle.com/java/technologies/downloads/?er=221886</downloadUrl>
  <supportUrl>https://www.oracle.com/java/technologies/downloads/?er=221886</supportUrl>
  <stayAlive>false</stayAlive>
  <restartOnCrash>false</restartOnCrash>
  <manifest></manifest>
  
  <!-- Ruta relativa al icono -->
  <icon>src\Imagenes\logo.ico</icon>
  
  <classPath>
    <mainClass>org.eclipse.jdt.internal.jarinjarloader.JarRsrcLoader</mainClass>
    <cp>.</cp>
  </classPath>
  <jre>
    <path>%JAVA_HOME%;%PATH%</path>
    <requiresJdk>true</requiresJdk>
    <requires64Bit>true</requires64Bit>
    <minVersion>1.8</minVersion>
    <maxVersion>22.0.1</maxVersion>
  </jre>
</launch4jConfig>
